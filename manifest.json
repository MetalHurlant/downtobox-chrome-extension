{
  "name": "Downtobox",
  "description": "Download uptobox content",
  "version": "0.1",
  "minimum_chrome_version": "31",
  "manifest_version": 2,
  "permissions": [
    "downloads",
    "storage"
  ],
  "browser_action": {
    "default_popup": "/views/popup.html"
  },
  "content_scripts": [ {
    "matches": [ "http://*/*", "https://*/*"],
    "js": [ "/lib/jquery.min.js" ]
  }],
  "options_page": "/views/options.html"
}
